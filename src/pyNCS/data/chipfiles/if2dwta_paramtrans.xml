<?xml version="1.0" encoding="UTF-8"?>
<paramtrans version="0.1" chipclass="IFSLWTA">
    <field id="constants">
        <parameter id="Cap" value="1.0569e-12"/>
        <parameter id="v_init"     value="0"/>
        <parameter id="v_reset"    value="0"/>
        <parameter id="Vdd"        value="3.3"/>
        <parameter id="gnd"        value="0"/>
        <parameter id="Kappap"     value="0.6904"/>
        <parameter id="Kappan"     value="0.757"/>
        <parameter id="Ut"         value="0.0261"/>
        <parameter id="I0p"        value="3.1226e-16"/>
        <parameter id="I0n"        value="5.5122e-14"/>
    </field>

    <field id="defaultparameters"> 
        <parameter id="w_synloc_S"         value="0.0"/>
        <parameter id="w_synloc_E"         value="0.0"/>
        <parameter id="w_synloc_EE"        value="0.0"/>
        <parameter id="w_syn_E1"           value="0.0"/>
        <parameter id="w_syn_E2"           value="0.0"/>
        <parameter id="w_syn_I"            value="0.0"/>
        <parameter id="i_inj"              value="0.0"/>
        <parameter id="i_leak"             value="0.0"/>
        <parameter id="v_thresh"           value="1100.0"/>
    </field> 
        
    <field id="translationnames">
        <name standard="w_synloc_S"          native="nsynlocselfw"/>
        <name standard="w_synloc_E"          native="nsynlocvert1"/>
        <name standard="w_synloc_EE"         native="nsynlocvert2"/>
        <name standard="w_syn_E1"            native="psynaerexcw0"/>
        <name standard="w_syn_E2"            native="psynaerexcw1"/>
        <name standard="w_syn_I"             native="psynaerinhth"/>
        <name standard="i_inj"               native="inj"/>
        <name standard="i_leak"              native="nlk"/>
        <name standard="v_thresh"            native="nsf"/>
    </field>

    <field id="calibration"> 
        <parameter id= "inj_fit"                 value="3.1226e-16"/>
        <parameter id= "nlk_fit"                 value="5.5122e-14"/>
        <parameter id= "nsynlocselfw_fit"        value="1.3778e-20"/>
        <parameter id= "nsynlocvert1_fit1"        value="1.3778e-20"/>
        <parameter id= "nsynlocvert1_fit2"        value="1."/>
        <parameter id= "nsynlocvert2_fit1"        value="1.3778e-18"/>
        <parameter id= "nsynlocvert2_fit2"        value="1."/>
        <parameter id= "psynaerexcw0_fit1"       value="2.1700e-20"/>
        <parameter id= "psynaerexcw1_fit1"       value="2.1700e-20"/>
        <parameter id= "psynaerexcw0_fit2"       value="0.7"/>
        <parameter id= "psynaerexcw1_fit2"       value="0.7"/>
        <parameter id= "psynaerinhth_fit1"        value="4.9190e-20"/>
        <parameter id= "psynaerinhth_fit2"        value="0.6"/>
    </field> 
        
    <field id="translationfunctions">
        <function standardname="w_synloc_S"     body="nsynlocselfw_fit*exp(Kappan*nsynlocselfw/Ut+Kappap*(-nsynlocth+psynloctau)/Ut)-w_synloc_S*1e-9"/>
        <function standardname="w_synloc_E"     body="nsynlocvert1_fit1*exp(nsynlocvert1_fit2*nsynlocvert1/Ut+Kappap*(-nsynlocth+psynloctau)/Ut)-w_synloc_E*1e-9"/>
        <function standardname="w_synloc_EE"    body="nsynlocvert2_fit1*exp(nsynlocvert2_fit2*nsynlocvert2/Ut+Kappap*(-nsynlocth+psynloctau)/Ut)-w_synloc_EE*1e-9"/>
        <function standardname="w_syn_E1"       body="psynaerexcw0_fit1*exp(Kappan*(psynaerexcw0_fit2*psynaerexcw0)/Ut+Kappap*((psynaerexctau0-Vdd)+(Vdd-nsynaerexcth0))/Ut)-w_syn_E1*1e-9"/>
        <function standardname="w_syn_E2"       body="psynaerexcw1_fit1*exp(Kappan*(psynaerexcw1_fit2*psynaerexcw1)/Ut+Kappap*((psynaerexctau1-Vdd)+(Vdd-nsynaerexcth1))/Ut)-w_syn_E2*1e-9"/>
        <function standardname="w_syn_I"        body="psynaerinhth_fit1*exp(psynaerinhth_fit2*(psynaerinhth-nsynaerinhtau)/Ut)-w_syn_I*1e-9"/>
        <function standardname="i_inj"          body="inj_fit*exp(Kappap*(Vdd-inj)/Ut)-i_inj*1e-9"/>
        <function standardname="v_thresh"       body="nsf+0.35-v_thresh*1e-3"/>
        <function standardname="i_leak"         body="nlk_fit*exp(Kappan*nlk/Ut)+0.8*Cap-i_leak*1e-9"/>
    </field>
</paramtrans>
